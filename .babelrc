{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "commonjs": {
      "plugins": [["@babel/plugin-transform-modules-commonjs"]]
    },
    "production": {
      "comments": false
    }
  }
}
