{
  "name": "bigdata-react-tendency-chart",
  "version": "0.1.6",
  "description": "用于react的趋势图组件",
  "private": false,
  "main": "lib/index",
  "module": "es6/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/qsz/bigdata-react-tendency-chart.git"
  },
  "files": [
    "demo",
    "dist",
    "es6",
    "lib",
    "src",
    "*.md"
  ],
  "keywords": [
    "bigdata",
    "react",
    "react-component",
    "chart"
  ],
  "scripts": {
    "build": "npm run build-common && npm run build-es6 && npm run build-dist",
    "build-common": "rimraf lib && cross-env BABEL_ENV=commonjs babel ./src --out-dir lib --extensions '.js,.jsx'",
    "build-es6": "rimraf es6 && cross-env BABEL_ENV=es6 babel ./src --out-dir es6 --extensions '.js,.jsx'",
    "build-dist": "rimraf dist && cross-env NODE_ENV=production webpack --color --progress",
    "demo": "webpack-dev-server --progress --port 8800 --host 127.0.0.1 --content-base demo --hot --inline --config demo/webpack.dev.config.js"
  },
  "author": "xjj745015549@163.com",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-brush": "^1.1.5",
    "d3-dispatch": "^1.0.6",
    "d3-ease": "^1.0.6",
    "d3-format": "^1.4.4",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-transition": "^1.3.2",
    "date-fns": "^2.13.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
